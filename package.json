{
  "name": "yaml-preview-ui",
  "displayName": "YAML Preview UI",
  "description": "VSCode extension for YAML visualization and editing",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:yaml",
    "onCommand:yaml-preview-ui.showPreview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "yaml-preview-ui.showPreview",
        "title": "Show YAML Preview"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorLangId == yaml",
          "command": "yaml-preview-ui.showPreview",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^12.11.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "buffer": "^6.0.3",
    "css-loader": "^6.5.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "process": "^0.11.10",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
